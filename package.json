{
  "name": "electron-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "dist/electron.js",
  "build": {
    "appId": "electron-react-webpack-boilerplate",
    "mac": {
      "category": "com.boilerplate.electron_react"
    },
    "win": {
      "iconUrl": "https://upload.wikimedia.org/wikipedia/commons/thumb/8/80/Atom_editor_logo.svg/1118px-Atom_editor_logo.svg.png"
    },
    "directories": {
      "app": "app",
      "buildResources": "build",
      "output": "release"
    }
  },
  "postinstall": "install-app-deps",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-server": "webpack --progress --hide-modules --config build/webpack.dev.conf.babel.js && rimraf static && node build/dev-server.js",
    "build": "rimraf app/dist && mkdirp app/dist && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.conf.babel.js",
    "build-dev": "rimraf dist && mkdirp dist && cross-env NODE_ENV=development webpack --progress --hide-modules --config build/webpack.dev.conf.babel.js",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundernet8/Electron-React-Webpack-Boilerplate.git"
  },
  "keywords": [
    "Electron",
    "React",
    "Redux",
    "React-Router",
    "Webpack",
    "Boilerplate",
    "Babel"
  ],
  "author": {
    "name": "WuXueqian",
    "email": "chinash2010@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thundernet8/Electron-React-Webpack-Boilerplate/issues"
  },
  "homepage": "https://github.com/thundernet8/Electron-React-Webpack-Boilerplate#readme",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^10.17.3",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.5.6",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "json": "^9.0.4",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "object.assign": "^4.0.4",
    "postcss": "^5.2.10",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "rebuild": "^0.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "scroll-behavior": "^0.9.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
